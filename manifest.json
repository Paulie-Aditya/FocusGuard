{
    "manifest_version": 3,
    "name": "URL Whitelist",
    "version": "1.0",
    "description": "Allows opening only specific URLs",
    "permissions": [
      "tabs",
      "storage"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  
  